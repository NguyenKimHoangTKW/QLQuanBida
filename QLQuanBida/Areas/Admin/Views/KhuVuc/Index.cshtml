        <td>
            @Html.DisplayFor(modelItem => item.MaKV)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TenKV)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.KVTinh)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SoLuong)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MoTa)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDKV }) |
